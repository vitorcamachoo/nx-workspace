{
  "version": 2,
  "projects": {
    "application-1": {
      "root": "apps/application-1",
      "sourceRoot": "apps/application-1",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/application-1",
            "outputPath": "dist/apps/application-1"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "application-1:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "application-1:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "application-1:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/application-1/**/*.{ts,tsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/application-1"],
          "options": {
            "jestConfig": "apps/application-1/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "@netlify-builder/deploy:deploy",
          "options": {
            "outputPath": "dist/apps/application-1/exported",
            "siteId": "c24578d2-61a6-47ef-9213-d0a20a0f3f78"
          }
        }
      }
    },
    "application-2": {
      "root": "apps/application-2",
      "sourceRoot": "apps/application-2/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/application-2",
            "index": "apps/application-2/src/index.html",
            "main": "apps/application-2/src/main.tsx",
            "polyfills": "apps/application-2/src/polyfills.ts",
            "tsConfig": "apps/application-2/tsconfig.app.json",
            "assets": [
              "apps/application-2/src/favicon.ico",
              "apps/application-2/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/application-2/src/environments/environment.ts",
                  "with": "apps/application-2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "application-2:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "application-2:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/application-2/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/application-2"],
          "options": {
            "jestConfig": "apps/application-2/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "@netlify-builder/deploy:deploy",
          "options": {
            "outputPath": "dist/apps/application-1/exported",
            "siteId": "c24578d2-61a6-47ef-9213-d0a20a0f3f78"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none"
      },
      "library": {
        "style": "none",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "application-1"
}
